{
  "name": "srhinow/bz-bbk-bundle",
  "description": "a tool to administration the 'bilderbuchkinos' (bbk) at bz-niedersachsen.de",
  "keywords": ["contao", "bbk","bilderbuchkinos"],
  "type": "contao-module",
  "license": "GPL-3.0+",
  "authors":[
    {
      "name":"Sven Rhinow",
      "homepage":"https://www.sr-tag.de",
      "role":"Developer"
    }
  ],
  "support":{
    "email": "kservice@sr-tag.de",
    "source":"git@gitlab.com:srhinow/bz-bbk-bundle.git"
  },
  "require": {
    "php": ">=7.2",
    "contao/core-bundle": "~3.5 || ~4.4",
    "contao-community-alliance/composer-plugin":"~2.4 || ~3.0",
    "dompdf/dompdf": "^0.8.0",
    "hschottm/tags": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Srhinow\\BzBbkBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "Srhinow\\BzBbkBundle\\ContaoManager\\Plugin"
  }
}
